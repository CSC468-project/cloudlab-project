pipeline {
    agent {
		kubernetes {
			defaultContainer 'buildct'
			yamlFile 'kubesbuilder.yaml'
		}
	}

    stages {
        stage('Deploy') {
			steps {
				echo 'Replacing current cloudcooked service'
                sh 'kubectl --namespace="default" replace --force -f cloudcooked.yaml'
			}
	    }
    }	
}
